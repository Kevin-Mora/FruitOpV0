<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fruitopv0">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FruitOpV0">
        <activity
            android:name=".PuntuacionesActivity"
            android:label="@string/title_activity_puntuaciones"
            android:theme="@style/Theme.FruitOpV0.NoActionBar"></activity>
        <activity android:name=".NivelesActivity" />
        <activity
            android:name=".ConfiguracionesActivity"
            android:label="@string/title_activity_configuraciones" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>